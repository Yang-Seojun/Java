package sec02.exam03;

public class ForMultipcationTableExample {
	public static void main(String[] args) {
		for(int m = 2; m <= 9; m++) {
			System.out.println("***" + m + "***");
			
			for(int n = 1; n <= 9;) {
				int result = m * n;
				System.out.println(m + " * " + n + " = " + result);
			}
		}
	}
}
